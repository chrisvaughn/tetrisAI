tell application "System Events"	-- Get the process for the application	set appProcess to first process whose name is "Nestopia"	-- Get the window of the process	tell appProcess		set frontmost to true		set windowList to windows		if (count of windowList) > 0 then			set theWindow to item 1 of windowList			-- Resize the window (example dimensions: 800x600)			set size of theWindow to {400, 400}			-- Optionally, you can also reposition the window			-- set position of theWindow to {100, 100}		else			display dialog "No windows found for Nestopia." buttons {"OK"} default button "OK"		end if	end tellend tell